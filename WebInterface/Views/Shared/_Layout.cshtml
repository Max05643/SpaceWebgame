<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="~/lib/bootstrap/css/bootstrap.min.css" asp-append-version="true">
    <link href="~/css/style.css" asp-append-version="true" rel="stylesheet">
    @await RenderSectionAsync("Style", required: false)
    @await RenderSectionAsync("ScriptsHead", required: false)
</head>
<body class="star-background text-light" style="width:100%;height:100%;overflow:hidden;">
    <header>
        <nav>
            <div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main>
            @RenderBody()
        </main>
    </div>

    <footer>
        <div>
            
        </div>
    </footer>
    <script src="~/lib/bootstrap/js/bootstrap.bundle.min.js" asp-append-version="true"></script>
    <script src="~/js/shared.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
